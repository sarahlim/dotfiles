{
  parser: 'babel-eslint',
  parserOptions: {
    ecmaVersion: 6,
    sourceType: 'module',
    ecmaFeatures: {
      jsx: true,
    },
  },
  extends: [
    'eslint:recommended',
    'plugin:import/errors',
  ],
  plugins: [
    'import',
    'flowtype',
  ],
  env: {
    browser: true,
    es6: true,
    node: true,
    jsx: true,
  },
}
